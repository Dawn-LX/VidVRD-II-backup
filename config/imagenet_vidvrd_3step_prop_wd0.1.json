{
    "dataset": "imagenet-vidvrd",
    "train_split": "train",
    "test_split": "test",
    "rng_seed": 1701,
    "model": {
        "name": "iterative_classifier",
        "hidden_dim": 768,
        "dropout": 0.2,
        "use_knowledge": false,
        "weight_decay": 0.1,
        "zero_shot_preference": "global_median",
        "dump_file": "weights_ep50.t7"
    },
    "training_poi_threshold": 0.9,
    "training_negative_poi_threshold": 0.5,
    "training_time_overlap_threshold": 20,
    "training_ignore_segments_wo_gt": true,
    "training_use_gt_label": true,
    "training_use_gt_tracklet": true,
    "training_focal_gamma": 2,
    "training_batch_size": 12,
    "training_max_cand_per_sample": 64,
    "training_lr": 0.001,
    "training_weight_decay": 0.0005,
    "training_max_epoch": 50,
    "training_display_freq": 1,
    "training_save_freq": 10,
    "training_n_workers": 12,
    "inference_steps": 3,
    "inference_problistic": true,
    "inference_temporal_propagate_threshold": 0.7,
    "inference_topk": 100,
    "inference_object_conf_threshold": 0.2,
    "inference_predicate_conf_threshold": 0.2,
    "inference_nms": 0.9,
    "inference_n_workers": 12,
    "association_algorithm": "greedy",
    "association_linkage_threshold": 0.5,
    "association_nms": 1.0,
    "association_topk": 200,
    "association_n_workers": 12
}
